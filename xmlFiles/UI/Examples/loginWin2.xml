<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="pyramid">
    <!--Set Env Variable: -->
    <!--QA-->
    <!--DEV-->
    <parameter name="env" value="Win2"/>

    <parameter name="isMobile" value="FALSE"/>
    <parameter name="isLinuxMachine" value="FALSE"/>

    <!--Set Portal Variable: -->
    <parameter name="portal" value="Pyramid"/>

    <test name="Automation Tests">
        <!--Set Browser Variable:-->
        <!--CHROME-->
        <!--FF-->
        <!--EDGE-->
        <!--IE-->
        <parameter name="browserType" value="CHROME"/>
        <classes>
            <class name="com.pyramid.tests.ui.win2Test"/>
        </classes>
    </test>
</suite>
